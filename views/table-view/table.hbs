<table class="table">
  <thead>
     
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last name</th>
      <th scope="col">e-mail</th>
      <th scope="col">password</th>
    </tr>
  </thead>
  <tbody>
       {{#each details}}
    <tr>
      <th scope="row">{{this.first_name}}</th>
      <td>{{this.last_name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.pw}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>